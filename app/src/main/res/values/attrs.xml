<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="circleview">
        <attr name="circle_background" format="color" />
        <attr name="circle_radius" format="dimension" />
        <attr name="stroke_width" format="dimension" />
        <attr name="increasing_offset" format="dimension" />
        <attr name="animation_duration" format="float" />
        <attr name="icon_offset_by_center_in_radius" format="float" />
        <attr name="min_click_time_in_mills" format="integer" />
        <attr name="max_click_time_in_mills" format="integer" />
    </declare-styleable>
</resources>