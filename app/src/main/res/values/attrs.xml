<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="circleview">
        <attr name="circle_background" format="color" />
        <attr name="circle_radius" format="dimension" />
        <attr name="stroke_width" format="dimension"/>
        <attr name="sector_amount" format="integer"/>
    </declare-styleable>

    <declare-styleable name="sector_view">
        <attr name="rotate_angle" format="integer"/>
        <attr name="center_angle" format="integer" />
        <attr name="sector_background" format="color"/>
        <attr name="iconic" format="integer"/>
        <attr name="text" format="string"/>
        <attr name="radius" format="dimension"/>
    </declare-styleable>

</resources>